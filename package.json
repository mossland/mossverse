    "start": "nx serve",
    "uploadEnv": "chmod +x infra/_general/jenkins/uploadEnv.sh && chmod +x infra/_general/jenkins/getSecrets.sh && bash infra/_general/jenkins/uploadEnv.sh",
    "downloadEnv": "chmod +x infra/_general/jenkins/downloadEnv.sh && chmod +x infra/_general/jenkins/getSecrets.sh && bash infra/_general/jenkins/downloadEnv.sh",
